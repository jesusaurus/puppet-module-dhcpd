subnet <%= scope.lookupvar('network') %> netmask <%= scope.lookupvar('netmask') %> {
	option domain-name-servers <% if scope.lookupvar('dns').respond_to?('join') %><%= scope.lookupvar('dns').join(', ') %><% else %><%= scope.lookupvar('dns') %><% end %>;
	option routers <%= scope.lookupvar('router') %>;

